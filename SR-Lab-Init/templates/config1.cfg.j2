        connector
            breakout c4-10g
        exit
        no shutdown
    exit
    port 1/1/c5
        connector
            breakout c4-10g
        exit
        no shutdown
    exit
    port 1/1/c6
        shutdown
    exit
    port 1/1/c7
        shutdown
