    port 1/1/c8
        shutdown
    exit
    port 1/1/c9
        shutdown
    exit
    port 1/1/c10
        shutdown
    exit
    port 1/1/c11
        shutdown
    exit
    port 1/1/c12
        shutdown
    exit
    port 1/2/c1
        shutdown
    exit
    port 1/2/c2
        shutdown
    exit
    port 1/2/c3
        shutdown
    exit
    port 1/2/c4
